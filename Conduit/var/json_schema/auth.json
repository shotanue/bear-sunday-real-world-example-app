{
  "$id": "auth.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Auth",
  "type": "object",
  "properties": {
    "uuid": {
      "type": "string",
      "description": "The uuid of the user",
      "maxLength": 255
    },
    "email": {
      "type": "string",
      "description": "The email of the user",
      "format": "email"
    }
  },
  "required": [
    "email",
    "uuid"
  ]
}
